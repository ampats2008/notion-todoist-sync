{
    "name": "todoist-notion-sync",
    "version": "1.0.0",
    "description": "A serverless function for syncing Todoist tasks with a Notion database. This repo contains portions of my private project that I have chosen to make public.",
    "main": "index.js",
    "scripts": {
        "dev-offline": "serverless offline start",
        "debug": "node --inspect node_modules/serverless/bin/serverless offline -s dev",
        "dev": "nodemon",
        "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/src/index.js",
        "build": "tsc -p tsconfig.prod.json",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "keywords": [],
    "author": "Anthony Medugno",
    "license": "MIT",
    "devDependencies": {
        "@types/aws-lambda": "^8.10.114",
        "@types/dotenv-safe": "^8.1.2",
        "@types/jest": "^29.5.0",
        "@types/node": "^18.15.10",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "dotenv": "^16.0.3",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.5.0",
        "lint-staged": "^13.2.0",
        "nodemon": "^2.0.12",
        "notion-types": "^6.16.0",
        "notion-utils": "^6.16.0",
        "prettier": "^2.3.2",
        "serverless": "^3.29.0",
        "serverless-functions-base-path": "^1.0.33",
        "serverless-offline": "^12.0.4",
        "serverless-plugin-typescript": "^2.1.4",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.1.0",
        "tsconfig-paths": "^4.1.2",
        "typescript": "^5.0.2"
    },
    "dependencies": {
        "@notionhq/client": "^2.2.3",
        "aws-sdk": "^2.1343.0",
        "dotenv-safe": "^8.2.0"
    }
}
